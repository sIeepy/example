.row
  .col-lg-8.col-lg-offset-2
    .text-center
      =render 'search'
    %table.table.table-striped
      %thead
        %tr
          %th.text-center #
          %th.text-center User
      .text-center
        %tbody
          - users.each_with_index do |user, i|
            %tr.text-center
              %td= i+1
              %td= link_to user.name, user, :class => "list_color"
